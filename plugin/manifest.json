{
  "name": "Youtube RSS",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This plugin adds the RSS button back to Youtube.",
  "homepage_url": "https://degrandis.github.io/Youtube-rss-chrome-plugin/",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
      "webNavigation",
      "https://www.youtube.com/watch?v=*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "css":[
          "css/styles.css"
      ],
      "js": [
        "js/jquery/jquery.js",
        "src/inject/contentScript.js"
      ]
    }
  ]
}
